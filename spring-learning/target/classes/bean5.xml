<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user1" class="top.lilong.spring.ioc.assemble.User">
        <constructor-arg index="0" value="tom"/>
        <constructor-arg index="1" value="22222"/>
        <constructor-arg index="2">
            <list>
                <value>constructor value1</value>
                <value>constructor value2</value>
            </list>
        </constructor-arg>
    </bean>
    <bean id="user2" class="top.lilong.spring.ioc.assemble.User">
        <property name="username" value="张三"/>
        <property name="password" value="123456"/>
        <property name="list">
            <list>
                <value>setter value1</value>
                <value>setter value2</value>
            </list>
        </property>
    </bean>
</beans>